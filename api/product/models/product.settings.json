{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": ["Available", "Unavailable"],
      "default": "Available"
    },
    "price": {
      "type": "float"
    },
    "images": {
      "collection": "product-image",
      "via": "products",
      "dominant": true
    }
  }
}
