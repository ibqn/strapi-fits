{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Available",
        "Unavailable"
      ],
      "default": "Available"
    },
    "price": {
      "type": "float"
    }
  }
}
